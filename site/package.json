    "eslint-plugin-svelte3": "^3.2.0",
    "hastscript": "^7.0.1",
    "mdsvex": "^0.9.3",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rehype-autolink-headings": "^5.1.0",
    "rehype-slug": "^4.0.1",
    "svelte": "^3.38.2",
    "vite": "^2.2.4"
  }
